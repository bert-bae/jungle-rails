<section class="admin-products-index">

  <header class="page-header">
    <h1>Admin &raquo; All Categories</h1>
  </header>

<h1>Signup!</h1>

<%= form_for :user, url: '/users' do |f| %>

  <div class="register-form col-sm-6">
    <label>First Name: </label><%= f.text_field :first_name %><br>
    <label>Last Name: </label><%= f.text_field :last_name %><br>
    <label>Email: </label><%= f.text_field :email %><br>
    <label>Password: </label><%= f.password_field :password %><br>
    <label>Password Confirmation: </label><%= f.password_field :password_confirmation %><br>
    <%= f.submit "Submit" %>
  </div>

<% end %>

</section>